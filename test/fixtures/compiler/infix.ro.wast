(module
  (memory 0 1)
  (export "testInt" (func $test::testInt))
  (export "testFloat" (func $test::testFloat))
  (func $test::sum (param $a f32) (param $b f32) (result f32)
    (f32.add (get_local $a) (get_local $b))
  )
  (func $test::sum_1 (param $a i32) (param $b i32) (result i32)
    (i32.add (get_local $a) (get_local $b))
  )
  (func $test::testInt (param $a i32) (param $b i32) (result i32)
    (call $test::sum_1 (get_local $a) (get_local $b))
  )
  (func $test::testFloat (param $a f32) (param $b f32) (result f32)
    (call $test::sum (get_local $a) (get_local $b))
  )
)
