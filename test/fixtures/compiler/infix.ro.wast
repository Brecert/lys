(module
  (export "testInt" (func $testInt))
  (export "testFloat" (func $testFloat))
  (func $sum (param $a f32) (param $b f32) (result f32)
    (f32.add (get_local $a) (get_local $b))
  )
  (func $sum1 (param $a i32) (param $b i32) (result i32)
    (i32.add (get_local $a) (get_local $b))
  )
  (func $testInt (param $a i32) (param $b i32) (result i32)
    (call $sum1 (get_local $a) (get_local $b))
  )
  (func $testFloat (param $a f32) (param $b f32) (result f32)
    (call $sum (get_local $a) (get_local $b))
  )
)
