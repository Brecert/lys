  "bugs": {
    "url": "https://github.com/menduz/aureum/issues"
  },
  "homepage": "https://github.com/menduz/aureum#readme",
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "printWidth": 120
  },
  "dependencies": {
    "colors": "^1.2.1",
    "ebnf": "^1.6.2"
  },
  "devDependencies": {
    "@types/expect": "^1.20.3",
    "@types/glob": "^5.0.35",
    "expect": "^22.4.3",
    "glob": "^7.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.1.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
